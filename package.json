{
  "name": "par-ui",
  "version": "0.0.62",
  "description": "Parmais React Components Library",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "start": "yarn dev:wds",
    "dev:wds": "webpack-dev-server --progress",
    "dev:build": "rimraf build && yarn dev:babel && yarn dev:scss && yarn build:copy-files",
    "dev:babel": "babel src -d build --ignore .spec.js",
    "dev:scss": "node-sass ./src/style/css/index.css ./build/index.css",
    "prod:build": "rimraf build && yarn prod:babel && yarn prod:scss && yarn build:copy-files",
    "prod:babel": "cross-env BABEL_ENV=production babel src -d build --ignore .spec.js",
    "prod:scss": "node-sass ./src/style/css/index.css ./build/style/css/index.css --output-style compressed",
    "build:copy-files": "babel-node ./config/copy-files.js",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "lint:fix": "eslint src webpack.config.babel.js --ext .js,.jsx --fix",
    "test": "yarn lint && jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/par-mais-tecnologia/par-ui.git"
  },
  "keywords": [
    "react",
    "component",
    "library"
  ],
  "author": "Parmais Tecnologia",
  "license": "(Apache-2.0)",
  "bugs": {
    "url": "https://github.com/par-mais-tecnologia/par-ui/issues"
  },
  "homepage": "https://github.com/par-mais-tecnologia/par-ui#readme",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "moment": "^2.18.1",
    "react": "^15.4.2",
    "react-currency-input": "^1.2.6",
    "react-dom": "^15.4.2",
    "react-input-mask": "^0.8.0",
    "react-modal": "^1.7.7",
    "react-onclickoutside": "^5.11.1",
    "react-select": "^1.0.0-rc.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "6",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-preset-babili": "^0.0.12",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "copyfiles": "^1.2.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "svg-url-loader": "^2.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "fs-extra": "^2.1.2",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/examples",
      "<rootDir>/build"
    ]
  }
}
